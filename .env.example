# AWS Configuration for Constitution Learning Hub Deployment
# Copy this file to .env and fill in your actual AWS credentials

# AWS Access Credentials
AWS_ACCESS_KEY_ID=your-aws-access-key-id-here
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key-here
AWS_DEFAULT_REGION=us-east-1

# CloudFront Distribution ID (if you have one)
CLOUDFRONT_DISTRIBUTION_ID=your-distribution-id-here

# Domain Configuration
CUSTOM_DOMAIN=your-domain.com